{
  "data": [{
    "type": "button",
    "attr": {
      "class": "btn btn-outline btn-default",
      "directive": {
        "ng-click": "$parent.addRowFunc()"
      }
    },
    "labeltext": "新增"
  }, {
    "type": "button",
    "attr": {
      "class": "btn btn-outline btn-default",
      "directive": {
        "ng-click": "$parent.delRowFunc()"
      }
    },
    "labeltext": "删除"
  }, {
    "type": "button",
    "attr": {
      "class": "btn btn-outline btn-default",
      "directive": {
        "ng-click": "$parent.modifyRowFunc()"
      }
    },
    "labeltext": "修改"
  }, {
    "type": "button",
    "attr": {
      "class": "btn btn-outline btn-default",
      "directive": {
        "ng-click": "$parent.viewRowFunc()"
      }
    },
    "labeltext": "查看"
  }]
}